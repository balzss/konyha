{
  "name": "konyha",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "next:dev": "next dev",
    "next:build": "next build",
    "next:start": "next start",
    "next:lint": "next lint",
    "prisma:studio": "HOST=localhost prisma studio",
    "prisma:generate": "prisma generate",
    "prisma:push": "HOST=localhost prisma db push",
    "gql:generate": "graphql-codegen --config ./graphql/codegen.yml",
    "docker:manager": "docker compose up manager",
    "docker:manager:build": "docker compose up --no-deps --build manager",
    "docker:manager:entrypoint": "npx prisma db push && node server.js",
    "docker:db": "docker compose up db",
    "docker:sitegen": "docker compose up sitegen",
    "docker:sitegen:build": "docker compose up --no-deps --build sitegen",
    "sitegen:dev": "cd sitegen && yarn dev",
    "sitegen:watch": "cd sitegen && yarn watch",
    "sitegen:build": "cd sitegen && yarn build",
    "sitegen:prisma:generate": "prisma generate --schema=./sitegen/prisma/schema.prisma",
    "sitegen:prisma:studio": "prisma studio --schema=./sitegen/prisma/schema.prisma",
    "sitegen:prisma:push": "prisma db push --schema=./sitegen/prisma/schema.prisma",
    "sitegen:demo": "cd sitegen && yarn demo",
    "homepage:dev": "cd sitegen/homepage && yarn start",
    "homepage:build": "cd sitegen/homepage && yarn build",
    "tmux:dev": "tmux send-keys 'yarn next:dev' C-m  && tmux split-window -h && tmux send-keys 'yarn docker:db' C-m && tmux split-window -v && tmux send-keys 'yarn sitegen:dev' C-m",
    "gh-pages": "cd sitegen && yarn gh-pages",
    "test": "jest --watch"
  },
  "dependencies": {
    "@apollo/client": "^3.6.2",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@graphql-codegen/typescript-react-apollo": "^3.2.14",
    "@mui/icons-material": "^5.6.2",
    "@mui/material": "^5.6.2",
    "@next-auth/prisma-adapter": "^1.0.3",
    "@prisma/client": "3.13.0",
    "apollo-server-micro": "^3.7.0",
    "graphql": "^15.3.0",
    "micro": "^9.3.4",
    "micro-cors": "^0.1.1",
    "next": "12.1.5",
    "next-auth": "^4.10.3",
    "node-fetch": "2",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "tslib": "^2.4.0",
    "use-debounce": "^8.0.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.11.6",
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/typescript": "2.4.11",
    "@graphql-codegen/typescript-operations": "2.4.0",
    "@graphql-codegen/typescript-resolvers": "2.6.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/micro-cors": "^0.1.2",
    "@types/node": "^17.0.31",
    "@types/node-fetch": "^2.6.2",
    "@types/react": "18.0.5",
    "@types/react-dom": "18.0.1",
    "eslint": "8.13.0",
    "eslint-config-next": "12.1.5",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "prisma": "^3.14.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  }
}
